{
  "name": "tree-sitter-lux",
  "version": "0.0.1",
  "description": "Tree-sitter parser for the Lux programming language",
  "author": "Fabian Achammer",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "npm-watch": "^0.6.0",
    "tree-sitter-cli": "^0.15.8"
  },
  "watch": {
    "test": {
      "patterns": ["grammar.js", "corpus/"],
